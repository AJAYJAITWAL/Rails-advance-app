<%= form_with model: @user, url: users_path, method: :post, local: true do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>

  <%= f.label :email %><br>
  <%= f.text_field :email %><br><br>

  <%= f.label :phone %><br>
  <%= f.text_field :phone %><br><br>
  
  <%= f.label :address %><br>
  <%= f.text_field :address %><br><br>
  <%= f.submit %>

<% end %><br>
<%= link_to "Back", users_path %>